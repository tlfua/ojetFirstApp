  },
  "files": [
    "*",
    "!**/__tests__"
  ],
  "main": "cjs/index.js",
  "module": "es/index.js",
  "exports": {
    "./*": {
      "browser": "./es/*.js",
      "import": "./es/*.js",
      "require": "./cjs/*.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./types/*/index.d.ts",
        "./types/*.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "peerDependencies": {
    "preact": "10.15.0"
  }
}
