{
  "name": "@oracle/oraclejet-preact",
  "version": "15.1.2",
  "description": "Oracle JET preact based components",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "eslint-all": "yarn eslint 'src/**/*.{ts,tsx}'",
    "dev": "cross-env SB_PORT=6006 yarn storybook",
    "docs": "cross-env STORIES=docs SB_PORT=6007 yarn storybook",
    "build-dev": "cross-env yarn build-storybook",
    "build-docs": "cross-env STORIES=docs yarn build-storybook",
    "build": "isNewer src ../oraclejet-cssprops/src ../pattern-library/src dist || exit 0 && yarn do-build && yarn remove-jira",
    "build-test": "yarn build",
    "do-build": "yarn eslint-all && yarn clean && yarn rollup && yarn build-index && yarn copy-pkg && yarn copy-images && yarn copy-sb-static || (yarn rename-dist && exit 1)",
    "build-index": "rollup -c scripts/rollup.index.js",
    "rename-dist": "rimraf dist.failed && renamer -s --find dist --replace dist.failed dist",
    "copy-pkg": "copyfiles -f package.json README.md THIRDPARTYLICENSE.txt dist",
    "copy-images": "copyfiles -f src/**/images/** dist/amd/images && copyfiles -f src/**/images/** dist/es/images",
    "copy-sb-static": "copyfiles -f src/**/images/** built/sb-static/images",
    "build-watch": "rollup -c scripts/rollup.config.js --watch",
    "clean-l10n": "rimraf src/resources/nls",
    "build-l10n": "yarn clean-l10n && yarn node scripts/build-l10n.js --rootDir=src/resources/icu --bundleName=bundle.json --locale=en --outDir=src/resources/nls && prettier --write 'src/resources/nls/**/*.ts'",
    "build-icons-redwood": "rimraf src/UNSAFE_RedwoodIcons && svgr --config-file scripts/svgr.config.js --svgo-config scripts/svgo.config.js --out-dir src/UNSAFE_RedwoodIcons -- src/resources/icons/redwood && node scripts/postprocessIcons.js -r -m -d -g -i UNSAFE_RedwoodIcons",
    "build-icons-custom": "rimraf src/PRIVATE_Icons && svgr --config-file scripts/svgr.config.js --svgo-config scripts/svgo.config.js --out-dir src/PRIVATE_Icons -- src/resources/icons/custom && node scripts/postprocessIcons.js -m -d -i PRIVATE_Icons",
    "build-icons-themed": "rimraf src/PRIVATE_ThemedIcons && node scripts/buildThemedIcons.js && node scripts/postprocessIcons.js -m -d -i PRIVATE_ThemedIcons",
    "build-icons": "yarn build-icons-redwood && yarn build-icons-custom && yarn build-icons-themed",
    "build-icons-images-update": "node scripts/updateRedwoodImages.js",
    "clean-icons-images-update": "rimraf src/resources/icons/redwood-old && rimraf src/resources/icons/redwood-diff.json",
    "rollup": "node --max-old-space-size=16384 $(yarn bin rollup) -c scripts/rollup.config.js",
    "clean": "rimraf built dist dist.failed test_results",
    "package": "cd dist && npm pack",
    "test": "jest -c test/jest.config.js",
    "test-watch": "yarn test --watch",
    "test-browser": "yarn convert-libs && yarn karma-launcher test/karma.conf.js",
    "test-browser-watch": "yarn test-browser --watch",
    "convert-libs": "yarn convert-testing-libs built",
    "storybook": "cross-env-shell NODE_OPTIONS=--max-old-space-size=16384 \"npx sb dev -p $SB_PORT\"",
    "build-storybook": "cross-env-shell NODE_OPTIONS=--max-old-space-size=16384 \"npx sb build -o dist/storybook-static\"",
    "build-storybook-docs": "cross-env-shell NODE_OPTIONS=--max-old-space-size=16384 \"npx sb build --docs\"",
    "remove-jira": "node ../build/glob 'dist/**'",
    "type-check": "tsc -p . --noEmit"
  },
  "author": "",
  "license": "UPL-1.0",
  "keywords": [
    "oraclejet"
  ],
  "devDependencies": {
    "@babel/cli": "7.20.7",
    "@babel/core": "7.22.9",
    "@babel/plugin-proposal-object-rest-spread": "7.20.7",
    "@babel/plugin-syntax-flow": "7.18.6",
    "@babel/plugin-transform-modules-commonjs": "7.21.2",
    "@babel/plugin-transform-spread": "7.20.7",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-flow": "7.18.6",
    "@chiragrupani/karma-chromium-edge-launcher": "2.3.1",
    "@floating-ui/dom": "0.5.3",
    "@jest/globals": "27.5.1",
    "@linaria/babel-preset": "3.0.0-beta.15",
    "@linaria/core": "3.0.0-beta.4",
    "@linaria/rollup": "3.0.0-beta.7",
    "@linaria/shaker": "3.0.0-beta.7",
