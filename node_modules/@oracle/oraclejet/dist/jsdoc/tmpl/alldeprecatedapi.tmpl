      <tbody>
        <?js
            var self = this;
            var deprRows = obj.deprObjects;
            self.versionMapping = {};
            var getMajorVersionNumber = function(version){
              var arr = version.split(".");
              var majorVer = parseInt(arr[0]);
              return majorVer;
            }
            self.count = 0;
            var currVersion = env.opts.query.jetVersion;
            var currMajorVer = getMajorVersionNumber(currVersion);
            deprRows.forEach(function(deprObj) {
              self.count++;
              let impclass = "";
              if (getMajorVersionNumber(deprObj.since) < currMajorVer - 2){
                  impclass = 'class="important"';
              }
              ?>
              <tr>
                <td><?js= deprObj.kind ?> </td>
                <td><code><?js= deprObj.apiname ?></code></td>
                <td><?js= deprObj.target ?></td>
                <td><code><?js= deprObj.for ?></code></td>
                <td><?js= deprObj.value ?></td>
                <td <?js= impclass ?> ><code><?js= deprObj.since ?></code></td>
                <td><code><?js= deprObj.until ?></code></td>
                <td class="description"><?js= deprObj.description ?></td>
              </tr>
            <?js  
            }); ?>
