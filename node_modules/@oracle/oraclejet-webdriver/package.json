{
  "name": "@oracle/oraclejet-webdriver",
  "version": "15.1.2",
  "description": "Oracle JET WebDriverJS WebElements",
  "config": {
    "revision": "2023-11-02_05-11-27"
  },
  "scripts": {
    "clean": "rimraf built dist dist.failed test/built test_results",
    "build": "isNewer index.ts elements lib ../../built/metadata/components/allComponents.json dist || exit 0 && yarn do-build",
    "do-build": "yarn clean && yarn create-webelements && yarn copy && yarn compile && yarn doc || (yarn rename-dist && exit 1)",
    "rename-dist": "rimraf dist.failed && renamer -s --find dist --replace dist.failed dist",
    "compile": "tsc --outDir dist",
    "copy": "copyfiles * 'bin/**' 'elements/**' 'lib/**' dist/",
    "create-webelements": "jet-internal/generate-jet ../../built/metadata/components/allComponents.json && prettier --write '**/*.ts'",
    "doc": "typedoc --out dist/wdtsdoc --options typedoc/typedoc.json",
    "package": "cd dist && npm pack",
    "test": "jest -c test/jest.config.js"
  },
  "dependencies": {
    "@oracle/oraclejet": "15.1.2"
  },
  "devDependencies": {
    "@oracle/build": "workspace:^",
    "@oracle/oraclejet-test-utils": "workspace:^",
    "@types/selenium-webdriver": "4.1.2",
    "chai": "4.3.7",
    "copyfiles": "2.4.1",
    "fs-extra": "10.1.0",
    "jest": "27.5.1",
    "prettier": "2.6.2",
    "renamer": "^4.0.0",
    "rimraf": "3.0.2",
    "typedoc": "0.24.6",
    "typedoc-plugin-missing-exports": "^0.22.6",
    "typescript": "5.0.4",
    "typescript-parser": "^2.6.1"
  },
  "files": [
    "index.*",
    "tsconfig.json",
    "wdtsdoc/",
    "elements/",
    "lib/",
    "bin/"
  ],
  "bin": {
    "ojwd-generate": "./bin/generate"
  },
  "peerDependencies": {
    "selenium-webdriver": ">=3.6.0"
  }
}