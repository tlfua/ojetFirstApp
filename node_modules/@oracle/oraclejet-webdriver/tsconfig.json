{
  "compilerOptions": {
    "rootDir": ".",
    "alwaysStrict": true,
    "module": "commonjs",
    "target": "ES2021",
    "noImplicitAny": true,
    "sourceMap": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "types": ["node", "selenium-webdriver"],
    "paths": {
      "@oracle/oraclejet/*": ["./node_modules/@oracle/oraclejet/dist/types/*"]
    }
  },
  "include": [
    "index.ts",
    "elements/**/*.ts"
  ]
}
